<?xml version="1.0" encoding="utf-8"?>

<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">
    
    <xs:simpleType name="PercentFraction">
        <xs:annotation>
            <xs:documentation>A simple type that restricts the value to a float between 0 and 1
                (inclusive).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:float">
            <xs:maxInclusive value="1.0"/>
            <xs:minInclusive value="0.0"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:complexType name="DistanceUnitItem">
        <xs:all>
            <xs:element form="qualified" name="Value" type="xs:double">
                <xs:annotation>
                    <xs:documentation>             The distance value in meters.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="DefaultUnitFormat" type="xs:string">
                <xs:annotation>
                    <xs:documentation>             If the format parameter is not specified in a
                        UnitItem method that expects a format value,             then this
                        DefaultUnitFormat is used.             Note that this is "just a hint" which
                        unit of measurement should be used to present             the distance value
                        to the user, it does not change the unit of the Value-field (which is
                        always meters, irrespective of the content of this element).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="IsReciprocal" type="xs:boolean"/>
            <xs:element form="qualified" minOccurs="0" name="Origin" type="xs:double"/>
            <xs:element form="qualified" minOccurs="0" name="Direction" type="xs:integer"/>
        </xs:all>
        <xs:attribute form="unqualified" name="Id" type="xs:string"/>
    </xs:complexType>
    
    <xs:complexType name="DateTimeUnitItem">
        <xs:all>
            <xs:element form="qualified" name="Value" type="xs:double">
                <xs:annotation>
                    <xs:documentation>             The date time value in ticks.
                    </xs:documentation>
                </xs:annotation>
                
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="DefaultUnitFormat" type="xs:string">
                <xs:annotation>
                    <xs:documentation>             If the format parameter is not specified in a
                        UnitItem method that expects a format value,             then this
                        DefaultUnitFormat is used.             Note that this is "just a hint" which
                        unit of measurement should be used to present             the date-time
                        value to the user, it does not change the unit of the Value-field (which is
                        always 'ticks', irrespective of the content of this element).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="IsReciprocal" type="xs:boolean"/>
            <xs:element form="qualified" minOccurs="0" name="Origin" type="xs:double"/>
            <xs:element form="qualified" minOccurs="0" name="Direction" type="xs:integer"/>
        </xs:all>
        <xs:attribute form="unqualified" name="Id" type="xs:string"/>
    </xs:complexType>
    
    <xs:complexType name="TimeSpanUnitItem">
        <xs:all>
            <xs:element form="qualified" name="Value" type="xs:double">
                <xs:annotation>
                    <xs:documentation>             The time value in seconds.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="DefaultUnitFormat" type="xs:string">
                <xs:annotation>
                    <xs:documentation>             If the format parameter is not specified in a
                        UnitItem method that expects a format value,             then this
                        DefaultUnitFormat is used.             Note that this is "just a hint" which
                        unit of measurement should be used to present             the time value to
                        the user, it does not change the unit of the Value-field (which is
                        always 'ticks', irrespective of the content of this element).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="IsReciprocal" type="xs:boolean"/>
            <xs:element form="qualified" minOccurs="0" name="Origin" type="xs:double"/>
            <xs:element form="qualified" minOccurs="0" name="Direction" type="xs:integer"/>
        </xs:all>
        <xs:attribute form="unqualified" name="Id" type="xs:string"/>
    </xs:complexType>
    
    <xs:complexType name="PixelUnitItem">
        <xs:all>
            <xs:element form="qualified" name="Value" type="xs:double">
                <xs:annotation>
                    <xs:documentation>             The number of pixels (no unit).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="DefaultUnitFormat" type="xs:string">
                <xs:annotation>
                    <xs:documentation>             If the format parameter is not specified in a
                        UnitItem method that expects a format value,             then this
                        DefaultUnitFormat is used.             Note that this is "just a hint" which
                        unit of measurement should be used to present             the value 'number
                        of pixels' to the user, it does not change the unit of the Value-field
                        (which is             always 'number of pixels', irrespective of the content
                        of this element).           </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="IsReciprocal" type="xs:boolean"/>
            <xs:element form="qualified" minOccurs="0" name="Origin" type="xs:double"/>
            <xs:element form="qualified" minOccurs="0" name="Direction" type="xs:integer"/>
        </xs:all>
        <xs:attribute form="unqualified" name="Id" type="xs:string"/>
    </xs:complexType>
    
    <xs:attributeGroup name="SubsetBounds">
        <xs:attribute form="unqualified" name="StartX" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeX" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartY" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeY" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartZ" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeZ" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartC" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeC" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartT" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeT" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartS" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeS" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartR" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeR" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartI" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeI" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartM" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeM" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartH" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeH" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartB" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeB" type="xs:integer"/>
        <xs:attribute form="unqualified" name="StartV" type="xs:integer"/>
        <xs:attribute form="unqualified" name="SizeV" type="xs:integer"/>
        <xs:attribute form="unqualified" name="ValidX" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidY" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidZ" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidC" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidT" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidR" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidS" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidI" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidM" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidH" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidB" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="ValidV" type="xs:boolean"/>
    </xs:attributeGroup>
    
    <xs:complexType name="UnspecificElementList">
        <xs:sequence>
            <xs:any maxOccurs="unbounded" minOccurs="0" processContents="skip"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="FileInfo">
        <xs:attribute form="unqualified" name="FullName" type="xs:string" use="required"/>
    </xs:complexType>
    
    <xs:complexType name="Rect">
        <xs:attribute form="unqualified" name="X"/>
        <xs:attribute form="unqualified" name="Y"/>
        <xs:attribute form="unqualified" name="Width"/>
        <xs:attribute form="unqualified" name="Height"/>
    </xs:complexType>
    
    <xs:simpleType name="StringSize">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:complexType name="Custom">
        <xs:sequence>
            <xs:any/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="Point">
        <xs:attribute form="unqualified" name="X"/>
        <xs:attribute form="unqualified" name="Y"/>
    </xs:complexType>
    
    <xs:simpleType name="StringPoint">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    
    <xs:simpleType name="Brush">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    
    <xs:simpleType name="FontFamily">
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    
    <xs:simpleType name="PixelType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Gray8"/>
            <xs:enumeration value="Gray16"/>
            <xs:enumeration value="Bgr24"/>
            <xs:enumeration value="Bgra32"/>
            <xs:enumeration value="Gray32Float"/>
            <xs:enumeration value="Bgr48"/>
            <xs:enumeration value="Bgr96Float"/>
            <xs:enumeration value="Gray64ComplexFloat"/>
            <xs:enumeration value="Gray32Float"/>
            <xs:enumeration value="Bgr192ComplexFloat"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="TagType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="String"/>
            <xs:enumeration value="Integer"/>
            <xs:enumeration value="Double"/>
            <xs:enumeration value="Date"/>
            <xs:enumeration value="Boolean"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:attributeGroup name="ParameterAttributes">
        <xs:attribute form="unqualified" name="Status">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Unknown"/>
                    <xs:enumeration value="Dirty"/>
                    <xs:enumeration value="Pending"/>
                    <xs:enumeration value="Valid"/>
                    <xs:enumeration value="SuperValid"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:attributeGroup>
    
    <xs:complexType name="CustomAttributes">
        <xs:sequence>
            <xs:any maxOccurs="unbounded" minOccurs="0" processContents="skip"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="ApplicationTag">
        <xs:all>
            <xs:element form="qualified" name="TagType" type="TagType"/>
            <xs:element form="qualified" name="Name" type="xs:string"/>
            <xs:element form="qualified" name="Value" type="xs:string"/>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="ApplicationTags">
        <xs:annotation>
            <xs:documentation>Application Tags</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element form="qualified" maxOccurs="unbounded" name="Tag" type="ApplicationTag"/>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="Parameters">
        <xs:sequence>
            <xs:element form="qualified" maxOccurs="unbounded" name="Parameter">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute form="unqualified" name="Name" type="xs:string"
                                use="required"/>
                            <xs:attribute form="unqualified" name="Type" use="required">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="integer"/>
                                        <xs:enumeration value="string"/>
                                        <xs:enumeration value="double"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:attribute>
                            <xs:attribute form="unqualified" name="MaxLength" type="xs:integer"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    
    <xs:complexType name="StringParameter">
        <xs:simpleContent>
            <xs:extension base="xs:string">
                <xs:attributeGroup ref="ParameterAttributes"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    
    <xs:complexType name="IntegerParameter">
        <xs:simpleContent>
            <xs:extension base="xs:integer">
                <xs:attributeGroup ref="ParameterAttributes"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    
    <xs:complexType name="BooleanParameter">
        <xs:simpleContent>
            <xs:extension base="xs:boolean">
                <xs:attributeGroup ref="ParameterAttributes"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    
    <xs:complexType name="DoubleParameter">
        <xs:simpleContent>
            <xs:extension base="xs:double">
                <xs:attributeGroup ref="ParameterAttributes"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    
    <xs:complexType name="FileReference">
        <xs:attribute form="unqualified" name="href" type="xs:string" use="required"/>
    </xs:complexType>
    
    <xs:complexType name="MetadataListItem">
        <xs:all>
            <xs:element form="qualified" name="Value"/>
        </xs:all>
        <xs:attribute form="unqualified" name="DoOneTimeLocalization" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="TextLocalizeId" type="xs:string"/>
        <xs:attribute form="unqualified" name="Text" type="xs:string"/>
    </xs:complexType>
    
    <xs:complexType name="RangeMetadata">
        <xs:attribute form="unqualified" name="Minimum" type="xs:double"/>
        <xs:attribute form="unqualified" name="Maximum" type="xs:double"/>
        <xs:attribute form="unqualified" name="SmallChange" type="xs:double"/>
        <xs:attribute form="unqualified" name="LargeChange" type="xs:double"/>
    </xs:complexType>
    
    <xs:complexType name="CombinedMetadata">
        <xs:all>
            <xs:element form="qualified" name="List">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element form="qualified" maxOccurs="unbounded" name="Item"
                            type="MetadataListItem"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element form="qualified" name="Range" type="RangeMetadata"/>
        </xs:all>
        <xs:attribute form="unqualified" name="Key" type="xs:string"/>
    </xs:complexType>
    
    <xs:simpleType name="EventType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Invalid"/>
            <xs:enumeration value="Marker"/>
            <xs:enumeration value="TimerChange"/>
            <xs:enumeration value="BleachStart "/>
            <xs:enumeration value="BleachStop"/>
            <xs:enumeration value="AdcTrigger"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="ImageUnit">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Unknown"/>
            <xs:enumeration value="Concentration"/>
            <xs:enumeration value="Meter"/>
            <xs:enumeration value="PhotonCountImageDisplay"/>
            <xs:enumeration value="PerCent"/>
            <xs:enumeration value="PalMoleculeDensity"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:simpleType name="MoleculeType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Molecule"/>
            <xs:enumeration value="Fiducials"/>
            <xs:enumeration value="UserFiducials"/>
            <xs:enumeration value="Trajectory"/>
            <xs:enumeration value="Unindentified"/>
            <xs:enumeration value="Simulation"/>
        </xs:restriction>
    </xs:simpleType>
    
    <xs:complexType name="Position">
        <xs:all>
            <xs:element form="qualified" name="Frame" type="xs:long"/>
            <xs:element form="qualified" name="PositionX" type="xs:double"/>
            <xs:element form="qualified" name="PositionY" type="xs:double"/>
            <xs:element form="qualified" name="PositionZ" type="xs:double"/>
            <xs:element form="qualified" name="ChiSquare" type="xs:double"/>
            <xs:element form="qualified" name="Sigma" type="xs:double"/>
            <xs:element form="qualified" name="SigmaZ" type="xs:double"/>
            <xs:element form="qualified" name="Background" type="xs:double"/>
            <xs:element form="qualified" name="NumberPhotons" type="xs:double"/>
            <xs:element form="qualified" name="LocalizationPrecision" type="xs:double"/>
            <xs:element form="qualified" name="LocalizationPrecisionZ" type="xs:double"/>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="Molecule">
        <xs:all>
            <xs:element form="qualified" name="MoleculeIndex" type="xs:string"/>
            <xs:element form="qualified" name="MoleculeType" type="MoleculeType"/>
            <xs:element form="qualified" name="FirstFrame" type="xs:long"/>
            <xs:element form="qualified" name="Frames" type="xs:long"/>
            <xs:element form="qualified" name="Missing" type="xs:long"/>
            <xs:element form="qualified" name="NumberPhotons" type="xs:long"/>
            <xs:element form="qualified" name="PositionX" type="xs:double"/>
            <xs:element form="qualified" name="PositionY" type="xs:double"/>
            <xs:element form="qualified" name="PositionZ" type="xs:double"/>
            <xs:element form="qualified" name="LocalizationPrecision" type="xs:double"/>
            <xs:element form="qualified" name="LocalizationPrecisionZ" type="xs:double"/>
            <xs:element form="qualified" name="ChiSquare" type="xs:double"/>
            <xs:element form="qualified" name="Sigma" type="xs:double"/>
            <xs:element form="qualified" name="SigmaZ" type="xs:double"/>
            <xs:element form="qualified" name="Background" type="xs:double"/>
            <xs:element form="qualified" name="PositionListIndex" type="xs:string"/>
            <xs:element form="qualified" name="PositionListSize" type="xs:long"/>
            <xs:element form="qualified" name="Positions">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element form="qualified" maxOccurs="unbounded" name="Position"
                            type="Position"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="Plane">
        <xs:all>
            <xs:element form="qualified" name="Gain" type="xs:double"/>
            <xs:element form="qualified" name="Offset" type="xs:double"/>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="LsmChannelSetting">
        <xs:all>
            
            
            <xs:element form="qualified" name="Gamma" type="xs:long"/>
            
            <xs:element form="qualified" name="Brightness" type="xs:long"/>
            
            <xs:element form="qualified" name="Contrast" type="xs:long"/>
            <xs:element form="qualified" name="Knots">
                <xs:complexType>
                    <xs:all>
                        
                        
                        <xs:element form="qualified" name="KnotX" type="xs:long"/>
                        <xs:element form="qualified" name="KnotY" type="xs:long"/>
                    </xs:all>
                </xs:complexType>
            </xs:element>
            
            <xs:element form="qualified" name="Palette">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element form="qualified" maxOccurs="unbounded" name="Value"
                            type="xs:short"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            
            <xs:element form="qualified" name="ExcitationWavelength" type="xs:double"/>
            <xs:element form="qualified" name="GratingPeriod" type="xs:double"/>
            <xs:element form="qualified" name="GratingPixels" type="xs:double"/>
            <xs:element form="qualified" name="DiffractionEfficiency1" type="xs:double"/>
            <xs:element form="qualified" name="DiffractionEfficiency2" type="xs:double"/>
            <xs:element form="qualified" name="DiffractionEfficiency3" type="xs:double"/>
            
            <xs:element form="qualified" name="Molecules">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element form="qualified" maxOccurs="unbounded" name="Molecule"
                            type="Molecule"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            
            
            <xs:element form="qualified" name="ChannelColor" type="xs:double"/>
            <xs:element form="qualified" name="ChannelName" type="xs:string"/>
            <xs:element form="qualified" name="Planes">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element form="qualified" maxOccurs="unbounded" name="Plane" type="Plane"
                        />
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            
            <xs:element form="qualified" name="FactorI" type="xs:double"/>
            <xs:element form="qualified" name="OffsetI" type="xs:double"/>
            
            <xs:element form="qualified" name="UnitI" type="ImageUnit"/>
            
            <xs:element form="qualified" name="Offset" type="xs:long"/>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="Wavelength">
        <xs:all>
            <xs:element form="qualified" name="Intensity" type="xs:long"/>
            <xs:element form="qualified" name="Wavelength" type="xs:long"/>
            <xs:element form="qualified" name="StandardDeviation" type="xs:long"/>
            
            <xs:element form="qualified" name="NumberChannels" type="xs:long"/>
            <xs:element form="qualified" name="WavelengthStart" type="xs:double"/>
            <xs:element form="qualified" name="WavelengthEnd" type="xs:double"/>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="LsmCharacteristics">
        <xs:annotation>
            <xs:documentation>Characteristics</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element form="qualified" name="ImageName" type="xs:string"/>
            <xs:element form="qualified" name="Description" type="xs:string"/>
            <xs:element form="qualified" name="Notes" type="xs:string"/>
            
            <xs:element form="qualified" name="CreatedByUser" type="xs:string"/>
            
            <xs:element form="qualified" name="AcquisitionDateAndTime" type="xs:date"/>
            
            <xs:element form="qualified" name="OriginalScanData" type="xs:boolean"/>
            
            <xs:element form="qualified" name="Device" type="xs:string"/>
            
        </xs:all>
    </xs:complexType>
    
    <xs:element name="DisplaySetting" type="DisplaySetting"/>
    
    <xs:complexType name="DisplaySetting">
        <xs:all>
            <xs:element form="qualified" name="Channels">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0"
                            name="Channel" type="Channel"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element form="qualified" maxOccurs="1" minOccurs="0" name="ViewerRotations">
                <xs:annotation>
                    <xs:documentation>             Angles for the image or scene composite image
                        (index 0) and for individual scenes              (starting from index 1 for
                        the first scene).            </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:list itemType="xs:double"/>
                </xs:simpleType>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="ToneMapping" type="ToneMapping"/>
        </xs:all>
        <xs:attribute default="true" form="unqualified" name="MergeChannels" type="xs:boolean">
            <xs:annotation>
                <xs:documentation>True if channel-merge display is enabled.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute default="false" form="unqualified" name="ShowRangeIndicator" type="xs:boolean"/>
        <xs:attribute form="unqualified" name="HistogramRange">
            <xs:annotation>
                <xs:documentation>The bit range for the histogram display.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="8"/>
                    <xs:enumeration value="10"/>
                    <xs:enumeration value="12"/>
                    <xs:enumeration value="14"/>
                    <xs:enumeration value="16"/>
                    <xs:enumeration value="Auto"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    
    <xs:complexType name="Channel">
        <xs:annotation>
            <xs:documentation>Defines the display setting for a single channel.</xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element default="0.0" form="qualified" minOccurs="0" name="Low" type="xs:double">
                <xs:annotation>
                    <xs:documentation>The normalized low (=black) value of the mapping
                        range.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="1.0" form="qualified" minOccurs="0" name="High" type="xs:double">
                <xs:annotation>
                    <xs:documentation>The normalized high (=white) value of the mapping
                        range.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="1.0" form="qualified" minOccurs="0" name="Gamma" type="xs:double">
                <xs:annotation>
                    <xs:documentation>The gamma value to be applied to the mapping
                        range.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="false" form="qualified" minOccurs="0" name="IsAutoApplyEnabled"
                type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>             Indicates whether the command specified in
                        AutoApplyMode is applied each time a new image             subset is
                        selected (e.g. when the player is running).           </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="false" form="qualified" minOccurs="0" name="IsColorNaNMode"
                type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>             A value indicating whether this instance uses
                        color NaN mode.             This option affects only float images, which
                        contain NaN values.           </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="#00000000" form="qualified" minOccurs="0" name="ColorNaN"
                type="xs:string">
                <xs:annotation>
                    <xs:documentation>The color for display of NaN values in float
                        images.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="false" form="qualified" minOccurs="0"
                name="UseFixFloatImageMinMaxValues" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>             If "true" a fix histogramm range is used for
                        float images otherwise minimum and maximum             values are determined
                        from image data.             Range is determined by FixFloatImageMinValue
                        and FixFloatImageMaxValue.           </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="0.0" form="qualified" minOccurs="0" name="FixFloatImageMinValue"
                type="xs:double">
                <xs:annotation>
                    <xs:documentation>             The minimum value of fixed histogramm range for
                        float images.           </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="10.0" form="qualified" minOccurs="0" name="FixFloatImageMaxValue"
                type="xs:double">
                <xs:annotation>
                    <xs:documentation>             The maximum value of fixed histogramm range for
                        float images.           </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="MinMax" form="qualified" minOccurs="0" name="AutoApplyMode">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="BestFit"/>
                        <xs:enumeration value="MinMax"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element default="2.0" form="qualified" minOccurs="0" name="LowerBestFitThreshold"
                type="xs:double">
                <xs:annotation>
                    <xs:documentation>             The lower threshold for the BestFit operation.
                        The value is useful e.g. to skip large nearly black areas with some noise.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="0.1" form="qualified" minOccurs="0" name="UpperBestFitThreshold"
                type="xs:double">
                <xs:annotation>
                    <xs:documentation>             The upper threshold for the BestFit operation.
                        The value is useful e.g. to skip large white or nearly white areas.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="8" form="qualified" minOccurs="0" name="BitCountRange"
                type="xs:int">
                <xs:annotation>
                    <xs:documentation>             The bit range. (Default: 8)
                        BitCountRange and Pixeltype are only persisted as indication for the
                        'optimal' range for the histogram.             Necessary for import of
                        display setting for acquisition channels and for adjusting Low and High
                        values if the bit range              of the imported display setting does
                        not match the bit depth of the camera (setting) of the  acquisition channel.
                        Note:             To persist the histogram range selected by the user, use
                        the element HistogramRange of DisplaySetting.           </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="Gray8" form="qualified" minOccurs="0" name="PixelType"
                type="PixelType">
                <xs:annotation>
                    <xs:documentation>             The pixel type. (Default: Gray8)
                        BitCountRange and Pixeltype are only persisted as indication for the
                        'optimal' range for the histogram.             Necessary for import of
                        display setting for acquisition channels and for adjusting Low and High
                        values if the bit range              of the imported display setting does
                        not match the bit depth of the camera (setting) of the  acquisition channel.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="Mode">
                <xs:annotation>
                    <xs:documentation>The mode: can be Spline, Ramp or None. Default is
                        None.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Spline"/>
                        <xs:enumeration value="Ramp"/>
                        <xs:enumeration value="None"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="Points" type="xs:string">
                <xs:annotation>
                    <xs:documentation>             If mode is Spline, these are the Spline Points.
                        If mode is Ramp, these are the Points for the Ramp mode.             If mode
                        is None, there is supposed to be no Points node, i.e. Points are ignored.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="Description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>User-defined description of the channel.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="DyeName" type="xs:string"/>
            <xs:element form="qualified" minOccurs="0" name="ShortName" type="xs:string"/>
            <xs:element form="qualified" minOccurs="0" name="IlluminationType" type="xs:string"/>
            <xs:element form="qualified" minOccurs="0" name="DyeMaxEmission" type="xs:string"/>
            <xs:element form="qualified" minOccurs="0" name="DyeMaxExcitation" type="xs:string"/>
            <xs:element form="qualified" minOccurs="0" name="DyeId" type="xs:string"/>
            <xs:element form="qualified" minOccurs="0" name="DyeDatabaseId" type="xs:string"/>
            <xs:element default="#FFFFFFFF" form="qualified" minOccurs="0" name="Color"
                type="xs:string">
                <xs:annotation>
                    <xs:documentation>The color in which to display the channel if
                        ColorMode=Color.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="Color" form="qualified" minOccurs="0" name="ColorMode">
                <xs:annotation>
                    <xs:documentation>The color mode in which to display the
                        channel.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Indeterminate"/>
                        <xs:enumeration value="None"/>
                        <xs:enumeration value="Color"/>
                        <xs:enumeration value="Palette"/>
                        <xs:enumeration value="Dye"/>
                        <xs:enumeration value="Custom"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element default="#FFFFFFFF" form="qualified" minOccurs="0" name="OriginalColor"
                type="xs:string">
                <xs:annotation>
                    <xs:documentation>The original color of the channel, i.e. the color the channel
                        had on the most recent save event.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="true" form="qualified" minOccurs="0" name="IsSelected"
                type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>             If the DisplaySetting is in MergeChannels mode,
                        the channel is only displayed if IsSelected is true.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="dawn" form="qualified" minOccurs="0" name="PaletteName"
                type="xs:string">
                <xs:annotation>
                    <xs:documentation>             If ColorMode=Palette, the channel is displayed
                        with the lookup-table (LUT)             defined by the PaletteName.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="Palette">
                <xs:annotation>
                    <xs:documentation>             This optional element allows to embed a custom
                        palette in a Channel.             It is used only when the ColorMode is
                        Palette.              You must also ensure that the PaletteName of the
                        Channel is not one of the built-in palette names,             otherwise the
                        custom palette will be ignored. It is recommended to use a              URN
                        as the PaletteName of custom palettes.           </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:all>
                        <xs:element form="qualified" minOccurs="1" name="Blue"
                            type="xs:base64Binary">
                            <xs:annotation>
                                <xs:documentation>                   The Blue components of the
                                    palette RGB triples in their natural order encoded as a Base64
                                    string.                   For each palette entry there is one
                                    byte.                   Blue, Green, and Red must have the same
                                    length, usually 256 bytes.                 </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element form="qualified" minOccurs="1" name="Green"
                            type="xs:base64Binary">
                            <xs:annotation>
                                <xs:documentation>                   The Green components of the
                                    palette RGB triples in their natural order encoded as a Base64
                                    string.                   For each palette entry there is one
                                    byte.                   Blue, Green, and Red must have the same
                                    length, usually 256 bytes.                 </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element form="qualified" minOccurs="1" name="Red" type="xs:base64Binary">
                            <xs:annotation>
                                <xs:documentation>                   The Green components of the
                                    palette RGB triples in their natural order encoded as a Base64
                                    string.                   For each palette entry there is one
                                    byte.                   Blue, Green, and Red must have the same
                                    length, usually 256 bytes.                 </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:all>
                </xs:complexType>
            </xs:element>
            <xs:element default="1.0" form="qualified" minOccurs="0" name="Weight" type="xs:double">
                <xs:annotation>
                    <xs:documentation>The channel weight (ratio among all selected
                        channels).</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="ChannelUnit">
                <xs:annotation>
                    <xs:documentation>             The ChannelUnit contains scaling factor, offset
                        and unit for each image             channel. The data are currently used by
                        images with ion concentration             data. The display value is
                        calculated by              DisplayValue = Factor * PixelIntensity /
                        MaxPixelIntensity + Offset.              where "MaxPixelIntensity" is the
                        maximum possible pixel intensity for the             data type (4095 or
                        255).           </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:all>
                        <xs:element form="qualified" name="FactorI" type="xs:double">
                            <xs:annotation>
                                <xs:documentation>A new factor value for the
                                    channel.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element form="qualified" name="OffsetI" type="xs:double">
                            <xs:annotation>
                                <xs:documentation>A new offset value for the
                                    channel.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element form="qualified" name="UnitI">
                            <xs:annotation>
                                <xs:documentation>The Unit identifier.</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="Unknown"/>
                                    <xs:enumeration value="Concentration"/>
                                    <xs:enumeration value="Meter"/>
                                    <xs:enumeration value="PhotonCountImageDisplay"/>
                                    <xs:enumeration value="PerCent"/>
                                    <xs:enumeration value="PalMoleculeDensity"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element form="qualified" name="ChannelType">
                            <xs:annotation>
                                <xs:documentation>The channel type identifier.</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="PalHR"/>
                                    <xs:enumeration value="PalWidefield"/>
                                    <xs:enumeration value="SimHR"/>
                                    <xs:enumeration value="SimWidefield"/>
                                    <xs:enumeration value="SimDWF"/>
                                    <xs:enumeration value="AiryScanSum"/>
                                    <xs:enumeration value="AiryScanRawSr"/>
                                    <xs:enumeration value="AiryScanSr"/>
                                    <xs:enumeration value="AiryScanVp"/>
                                    <xs:enumeration value="AiryScanSr2D"/>
                                    <xs:enumeration value="Unspecified"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:all>
                </xs:complexType>
            </xs:element>
        </xs:all>
        <xs:attribute form="unqualified" name="Id" type="xs:string">
            <xs:annotation>
                <xs:documentation>           An identifier to uniquely identify this channel.
                    Use this Id to refer to the respective Channel node within
                    Dimensions/Channels/Channel.         </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="Name" type="xs:string">
            <xs:annotation>
                <xs:documentation>           The (user-defined) channel name.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="StartC" type="xs:integer" use="optional">
            <xs:annotation>
                <xs:documentation>           The index of the channel.           In general, the
                    channel elements in the channels collection are indexed by 0, 1, 2, etc.
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    
    <xs:complexType name="ToneMapping">
        <xs:all>
            <xs:element form="qualified" minOccurs="1" name="Mode" type="xs:string"> </xs:element>
            <xs:element form="qualified" minOccurs="1" name="EnhanceClipLimit" type="xs:string"> </xs:element>
            <xs:element form="qualified" minOccurs="1" name="EnhanceRegionSizePercentage"
                type="xs:string"> </xs:element>
        </xs:all>
    </xs:complexType>
    
    <xs:element name="Views" type="Views"/>
    
    <xs:complexType name="Views">
        <xs:sequence>
            <xs:element form="qualified" maxOccurs="unbounded" name="View" type="View"/>
        </xs:sequence>
    </xs:complexType>
    
    
    <xs:complexType name="View">
        <xs:all>
            <xs:element form="qualified" minOccurs="0" name="ViewAspect" type="ViewAspect"/>
            <xs:element form="qualified" minOccurs="0" name="DisplaySetting" type="DisplaySetting"/>
        </xs:all>
        <xs:attribute form="unqualified" name="Id" type="xs:string" use="required"/>
        <xs:attribute form="unqualified" name="Name" type="xs:string"/>
    </xs:complexType>
    
    <xs:complexType name="ViewAspect">
        <xs:all>
            <xs:element form="qualified" name="DimensionViews">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element form="qualified" maxOccurs="unbounded" name="DimensionView"
                            type="DimensionView"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element default="true" form="qualified" minOccurs="0" name="IsInterpolationEnabled"
                type="xs:boolean"/>
            <xs:element default="1.0" form="qualified" minOccurs="0" name="Zoom" type="xs:double"/>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="DimensionView">
        <xs:all>
            <xs:element form="qualified" name="Current" type="xs:integer"/>
            <xs:element form="qualified" minOccurs="0" name="Start" type="xs:integer"/>
            <xs:element form="qualified" minOccurs="0" name="Minimum" type="xs:integer"/>
            <xs:element form="qualified" minOccurs="0" name="Maximum" type="xs:integer"/>
        </xs:all>
        <xs:attribute form="unqualified" name="Id" use="required">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="X"/>
                    <xs:enumeration value="Y"/>
                    <xs:enumeration value="C"/>
                    <xs:enumeration value="Z"/>
                    <xs:enumeration value="T"/>
                    <xs:enumeration value="S"/>
                    <xs:enumeration value="I"/>
                    <xs:enumeration value="R"/>
                    <xs:enumeration value="H"/>
                    <xs:enumeration value="B"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    
    <xs:complexType name="Manufacturer">
        <xs:annotation>
            <xs:appinfo>abstract</xs:appinfo>
            <xs:documentation>         Manufacturer info to be added to various microscope
                components. E.g Objective, Filter etc.         Provides attributes for recording
                common properties of these components such as Manufacturer name, Model etc,
                all of which are optional.       </xs:documentation>
        </xs:annotation>
        <xs:all>
            <xs:element form="qualified" minOccurs="0" name="Manufacturer" type="xs:string">
                <xs:annotation>
                    <xs:documentation>The manufacturer of the component. [plain text
                        string]</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="Model" type="xs:string">
                <xs:annotation>
                    <xs:documentation>The Model of the component. [plain text
                        string]</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="SerialNumber" type="xs:string">
                <xs:annotation>
                    <xs:documentation>The serial number of the component. [plain text
                        string]</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="LotNumber" type="xs:string">
                <xs:annotation>
                    <xs:documentation>The lot number of the component. [plain text
                        string]</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:all>
    </xs:complexType>
    
    <xs:complexType name="Instrument">
