                            <xs:annotation>
                                <xs:documentation>                   Boolean that indicates whether
                                    the remove outlier filter is applied to the data.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element form="qualified" name="RemoveOutliersRadius" type="xs:double">
                            <xs:annotation>
                                <xs:documentation>                   Radius in nm (Search radius for
                                    finding the molecule neighbors).
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element form="qualified" name="RemoveOutliersThreshold"
                            type="xs:integer">
                            <xs:annotation>
                                <xs:documentation>                   Minimum number of molecule
                                    neighbours within the radius.
                                </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element form="qualified" name="LiveSettings">
                <xs:annotation>
                    <xs:documentation>             Parameter for the live processing settings
