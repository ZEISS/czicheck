                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="Detectors">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element form="qualified" maxOccurs="unbounded" name="Detector"
                            type="InstrumentDetector"/>
                    </xs:sequence>
                </xs:complexType>
