                    <xs:documentation>The Rating of the image document. Values:
                        0-5.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="Keywords" type="xs:string"/>
            <xs:element form="qualified" minOccurs="0" name="CustomAttributes"
                type="CustomAttributes"/>
        </xs:all>
    </xs:complexType>
    
