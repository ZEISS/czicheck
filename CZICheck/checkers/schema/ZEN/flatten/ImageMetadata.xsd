    <xs:element name="Information" type="Information"/>
    
    <xs:complexType name="Information">
        <xs:all>
            <xs:element form="qualified" minOccurs="0" name="Image" type="Image"/>
            <xs:element form="qualified" minOccurs="0" name="TimelineTracks" type="TimelineTracks"/>
            <xs:element form="qualified" minOccurs="0" name="User" type="User"/>
            <xs:element form="qualified" minOccurs="0" name="Institution" type="Institution"/>
            <xs:element form="qualified" minOccurs="0" name="Document" type="Document"/>
            <xs:element form="qualified" minOccurs="0" name="Application" type="Application"/>
            <xs:element form="qualified" minOccurs="0" name="Environment" type="Environment"/>
            <xs:element form="qualified" minOccurs="0" name="Processing" type="Processing"/>
            <xs:element form="qualified" minOccurs="0" name="Instrument" type="Instrument"/>
            <xs:element form="qualified" minOccurs="0" name="CustomAttributes"
                type="CustomAttributes"/>
            <xs:element form="qualified" minOccurs="0" name="Measurement">
                <xs:complexType>
                    <xs:sequence>
                        <xs:any maxOccurs="unbounded" minOccurs="0" processContents="skip"/>
                    </xs:sequence>
                    <xs:anyAttribute processContents="skip"/>
                </xs:complexType>
            </xs:element>
            <xs:element form="qualified" minOccurs="0" name="ViewerSettings">
                <xs:annotation>
                    <xs:documentation>             Contains user settings that are set in a specific
                        image viewer can be persisted in order to restore them when opening a
                        document in this specific viewer.           </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:sequence>
                        <xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax"/>
                    </xs:sequence>
                    <xs:anyAttribute processContents="skip"/>
