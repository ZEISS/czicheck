            <xs:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="TextElement">
                <xs:complexType>
                    <xs:all>
                        <xs:element default="false" form="qualified" minOccurs="0" name="IsEditable"
                            type="xs:boolean"/>
                        <xs:element default="false" form="qualified" minOccurs="0" name="IsWrapping"
                            type="xs:boolean"/>
                        <xs:element default="true" form="qualified" minOccurs="0" name="IsMovable"
                            type="xs:boolean">
                            <xs:annotation>
                                <xs:documentation>Specifies the posibility to move the absolute or
                                    the relative position.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element default="true" form="qualified" minOccurs="0"
                            name="IsAlignEnable" type="xs:boolean">
                            <xs:annotation>
                                <xs:documentation>Specifies if the text align with the properties
                                    horizontal and vertical alignment.</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element form="qualified" minOccurs="0" name="Text" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>                   The text can contain following
                                    wildcards:                   [m] measurement, [a] annotation,
                                    [i] displayId, [n] name, [t:TagName], ..                   The
                                    wildcards are replaced by the required graphicElement
                                    properties.                 </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        
                        <xs:element form="qualified" minOccurs="0" name="IsRotateWithParent"
                            type="xs:boolean"/>
                        <xs:element form="qualified" minOccurs="0" name="RenderedSize"
                            type="xs:string"/>
                        <xs:element form="qualified" minOccurs="0" name="Position" type="xs:string">
                            <xs:annotation>
                                <xs:documentation>Specify the top left position of the text. The
                                    position is set in absolute image coordinate. Format: "X1,Y1" or
                                    "X1,Y1,Z1"</xs:documentation>
                            </xs:annotation>
                        </xs:element>
                        <xs:element default="0.0" form="qualified" minOccurs="0" name="TextRotation"
                            type="xs:double">
                            <xs:annotation>
                                <xs:documentation>                   Specify the rotation of the
                                    text.                 </xs:documentation>
                            </xs:annotation>
                        </xs:element>
                    </xs:all>
                    <xs:attribute form="unqualified" name="Id">
                        <xs:simpleType>
