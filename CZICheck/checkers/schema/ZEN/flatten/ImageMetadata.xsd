                                <xs:enumeration value="RelativePosition.BottomLeft"/>
                                <xs:enumeration value="RelativePosition.BottomRight"/>
                                <xs:enumeration value="RelativePosition.Center"/>
                                <xs:enumeration value="AlignPosition"/>
                            </xs:restriction>
